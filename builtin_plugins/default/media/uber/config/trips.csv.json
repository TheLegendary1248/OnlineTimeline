{
    "$schema": "../../../json_schemas/csv_config_schema.json",
    "config": {
        "expectedHeader": [
            "City",
            "Product Type",
            "Trip or Order Status",
            "Request Time",
            "Begin Trip Time",
            "Begin Trip Lat",
            "Begin Trip Lng",
            "Begin Trip Address",
            "Dropoff Time",
            "Dropoff Lat",
            "Dropoff Lng",
            "Dropoff Address",
            "Distance (miles)",
            "Fare Amount",
            "Fare Currency"
        ],
        "conversions": {
            "(Fare Amount)|(.* (Lng|Lat))|(Distance.*)": {
                "type":"number"
            },
            ".* Time": {
                "isRegex": true,
                "type": "time",
                "config": {
                    "method":"strptime",
                    "format": "%Y-%m-%d %X %z %Z"
                }
            },
            "Trip or Order Status": {
                "type": "enum",
                "config": {
                    "values": {
                        "UNFULFILLED": 2,
                        "COMPLETED": 1,
                        "CANCELLED": 0
                    },
                    "keepOnKeyError": true
                }
            }
        }
    }
}